name: train_random_forest
conda_env: conda.yml

entry_points:
  main:
    parameters:
      trainval_artifact: {type: string}
      val_size: {type: string}
      random_seed: {type: string}
      stratify_by: {type: string}
      rf_config: {type: string}
      max_tfidf_features: {type: string}
      output_artifact: {type: string}
    command: "python run.py --trainval_artifact {trainval_artifact} --val_size {val_size} --random_seed {random_seed} --stratify_by {stratify_by} --rf_config {rf_config} --max_tfidf_features {max_tfidf_features} --output_artifact {output_artifact}"